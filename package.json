{
  "name": "npm-typescript-template",
  "version": "0.1.0",
  "description": "Template project for typescript and rollup",
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.mjs",
  "scripts": {
    "build:devtools": "tsc --project devtools/tsconfig.json",
    "pretest": "npm run build",
    "test": "jest",
    "prebuild": "npm run build:devtools",
    "build": "rollup -c",
    "clean": "rm -rf .rpt2_cache build lib devtools/build devtools/lib",
    "prepublishOnly": "npm run clean && NODE_ENV=production npm run build"
  },
  "bin": {
    "hello": "bin/hello.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BobKerns/npm-template.git"
  },
  "keywords": [
    "template",
    "node",
    "typescript",
    "rollup"
  ],
  "author": "Bob Kerns",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BobKerns/npm-template/issues"
  },
  "homepage": "https://github.com/BobKerns/npm-template#readme",
  "dependencies": {
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "wcsize": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "jest": "^24.8.0",
    "rollup": "^1.12.3",
    "rollup-plugin-terser": "^5.0.0",
    "rollup-plugin-typescript2": "^0.21.1",
    "rollup-plugin-visualizer": "^1.1.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.5"
  }
}
